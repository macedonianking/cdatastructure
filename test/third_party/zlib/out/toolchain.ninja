rule cc
  command = gcc -MMD -MF ${out}.d ${cflags} ${include_dirs} ${defines} ${cflags_c} -c ${in} -o ${out}
  description = CC ${out}
  depfile = ${out}.d
  deps = gcc
rule cxx
  command = g++ -MMD -MF ${out}.d ${cflags} ${include_dirs} ${defines} ${cflags_cc} -c ${in} -o ${out}
  description = CXX ${out}
  depfile = ${out}.d
  deps = gcc
rule alink
  command = D:/share/Python34/python.exe ../../../build/toolchain/gcc_ar_wrapper.py --depfile="${out}.d" --ar="ar" --resource-whitelist="${out}.whitelist" --output="${out}" ${arflags} rcsD @"${out}.rsp"
  description = ALINK ${label}
  rspfile = ${out}.rsp
  rspfile_content = ${in}
  depfile = ${out}.d
  deps = gcc
rule solink
  command = D:/share/Python34/python.exe "D:/dir/cdatastructure/test/build/toolchain/gcc_solink_wrapper.py" --nm="nm" --tocfile="${output_dir}/${target_output_name}${output_extension}.TOC" --soname="${target_output_name}${output_extension}" --sofile="${output_dir}/${target_output_name}${output_extension}" --output="${output_dir}/${target_output_name}${output_extension}" -- "g++ ${ldflags} -o ${output_dir}/${target_output_name}${output_extension} -Wl,-soname,${target_output_name}${output_extension} -Wl,--whole-archive ${in} ${solibs}  -Wl,--no-whole-archive ${libs}"
  description = SOLINK ${output_dir}/${target_output_name}${output_extension}
  pool = link_pool
rule link
  command = g++ ${ldflags} -o ${out} -Wl,--start-group ${in} ${solibs} -Wl,--end-group ${libs} ${solibs}
  description = LINK ${label}
  pool = link_pool
rule stamp
  command = touch ${out}
  description = STAMP ${out}
rule copy
  command = cp -f ${in} ${out}
  description = COPY ${out}

