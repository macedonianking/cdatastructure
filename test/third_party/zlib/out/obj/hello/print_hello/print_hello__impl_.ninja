defines = -DPRINT_HELLO_IMPLEMENTATION
include_dirs =
cflags = -Werror -O2 -g
cflags_cc = -std=c++0x
label = //hello/print_hello$:print_hello__impl_
label_name = print_hello__impl_
target_out_dir = obj/hello/print_hello
target_output_name = libprint_hello

build obj/hello/print_hello/print_hello__impl_/print_hello.o: cxx ../../../hello/print_hello/print_hello.cpp

build obj/hello/print_hello/libprint_hello.dll obj/hello/print_hello/libprint_hello.dll.TOC: solink obj/hello/print_hello/print_hello__impl_/print_hello.o
  ldflags = -shared
  libs =
  output_extension = .dll
  output_dir = obj/hello/print_hello
