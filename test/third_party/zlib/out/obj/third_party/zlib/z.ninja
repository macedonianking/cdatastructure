defines =
include_dirs = -I../../../third_party/zlib
cflags = -Werror -O2 -g
cflags_c = -std=gnu99
label = //third_party/zlib$:z
label_name = z
target_out_dir = obj/third_party/zlib
target_output_name = libz

build obj/third_party/zlib/z/adler32.o: cc ../../../third_party/zlib/adler32.c
build obj/third_party/zlib/z/compress.o: cc ../../../third_party/zlib/compress.c
build obj/third_party/zlib/z/crc32.o: cc ../../../third_party/zlib/crc32.c
build obj/third_party/zlib/z/deflate.o: cc ../../../third_party/zlib/deflate.c
build obj/third_party/zlib/z/gzclose.o: cc ../../../third_party/zlib/gzclose.c
build obj/third_party/zlib/z/gzlib.o: cc ../../../third_party/zlib/gzlib.c
build obj/third_party/zlib/z/gzread.o: cc ../../../third_party/zlib/gzread.c
build obj/third_party/zlib/z/gzwrite.o: cc ../../../third_party/zlib/gzwrite.c
build obj/third_party/zlib/z/infback.o: cc ../../../third_party/zlib/infback.c
build obj/third_party/zlib/z/inffast.o: cc ../../../third_party/zlib/inffast.c
build obj/third_party/zlib/z/inflate.o: cc ../../../third_party/zlib/inflate.c
build obj/third_party/zlib/z/inftrees.o: cc ../../../third_party/zlib/inftrees.c
build obj/third_party/zlib/z/trees.o: cc ../../../third_party/zlib/trees.c
build obj/third_party/zlib/z/uncompr.o: cc ../../../third_party/zlib/uncompr.c
build obj/third_party/zlib/z/zutil.o: cc ../../../third_party/zlib/zutil.c

build ./libz.a: alink obj/third_party/zlib/z/adler32.o obj/third_party/zlib/z/compress.o obj/third_party/zlib/z/crc32.o obj/third_party/zlib/z/deflate.o obj/third_party/zlib/z/gzclose.o obj/third_party/zlib/z/gzlib.o obj/third_party/zlib/z/gzread.o obj/third_party/zlib/z/gzwrite.o obj/third_party/zlib/z/infback.o obj/third_party/zlib/z/inffast.o obj/third_party/zlib/z/inflate.o obj/third_party/zlib/z/inftrees.o obj/third_party/zlib/z/trees.o obj/third_party/zlib/z/uncompr.o obj/third_party/zlib/z/zutil.o
  arflags =
  output_extension = .a
  output_dir = .
