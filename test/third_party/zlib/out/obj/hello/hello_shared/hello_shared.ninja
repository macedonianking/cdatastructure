defines = -DHELLO_SHARED_IMPLEMENTATION
include_dirs =
cflags = -Werror -O2 -g
cflags_c = -std=gnu99
label = //hello/hello_shared$:hello_shared
label_name = hello_shared
target_out_dir = obj/hello/hello_shared
target_output_name = libhello_shared

build obj/hello/hello_shared/hello_shared/hello_shared.o: cc ../../../hello/hello_shared/hello_shared.c

build ./libhello_shared.dll ./libhello_shared.dll.TOC: solink obj/hello/hello_shared/hello_shared/hello_shared.o
  ldflags = -shared
  libs =
  output_extension = .dll
  output_dir = .
