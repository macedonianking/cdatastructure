defines = -DBLAZE_OPENSOURCE
include_dirs = -I../../.. -I../../../third_party/zlib
cflags =
cflags_cc = -std=c++0x
label = //third_party/ijar$:ijar
label_name = ijar
target_out_dir = obj/third_party/ijar
target_output_name = ijar

build obj/third_party/ijar/ijar/classfile.o: cxx ../../../third_party/ijar/classfile.cc
build obj/third_party/ijar/ijar/dummy_zlib_client.o: cxx ../../../third_party/ijar/dummy_zlib_client.cc
build obj/third_party/ijar/ijar/ijar.o: cxx ../../../third_party/ijar/ijar.cc
build obj/third_party/ijar/ijar/platform_utils.o: cxx ../../../third_party/ijar/platform_utils.cc
build obj/third_party/ijar/ijar/zip.o: cxx ../../../third_party/ijar/zip.cc
build obj/third_party/ijar/ijar/zip_main.o: cxx ../../../third_party/ijar/zip_main.cc
build obj/third_party/ijar/ijar/zlib_client.o: cxx ../../../third_party/ijar/zlib_client.cc
build obj/third_party/ijar/ijar/mapped_file_windows.o: cxx ../../../third_party/ijar/mapped_file_windows.cc

build ./ijar.exe: link obj/third_party/ijar/ijar/classfile.o obj/third_party/ijar/ijar/dummy_zlib_client.o obj/third_party/ijar/ijar/ijar.o obj/third_party/ijar/ijar/platform_utils.o obj/third_party/ijar/ijar/zip.o obj/third_party/ijar/ijar/zip_main.o obj/third_party/ijar/ijar/zlib_client.o obj/third_party/ijar/ijar/mapped_file_windows.o ./libmain.a ./libz.a
  ldflags =
  libs =
  output_extension = .exe
  output_dir = .
