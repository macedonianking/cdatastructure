defines =
include_dirs = -I../../../hello/hello_static
cflags = -Werror -O2 -g
cflags_c = -std=gnu99
label = //hello/hello_static$:hello_static
label_name = hello_static
target_out_dir = obj/hello/hello_static
target_output_name = libhello_static

build obj/hello/hello_static/hello_static/hello_static.o: cc ../../../hello/hello_static/hello_static.c

build ./libhello_static.a: alink obj/hello/hello_static/hello_static/hello_static.o
  arflags =
  output_extension = .a
  output_dir = .
